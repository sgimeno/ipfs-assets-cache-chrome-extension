
{
  "name": "IPFS assets cache",
  "description": "This extension caches assets using IPFS",
  "version": "0.0.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enable IPFS cache",
    "default_icon": "assets/icon.png"
  },
  "manifest_version": 2
}
